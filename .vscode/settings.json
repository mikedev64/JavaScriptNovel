{
  "security.workspace.trust.untrustedFiles": "open",

  "terminal.integrated.gpuAcceleration": "on",

  "editor.tabSize": 8,
  "editor.fontSize": 14,
  "editor.indentSize": 8,
  "editor.cursorWidth": 3,
  "editor.wordWrap": "off",
  "editor.lineNumbers": "on",
  "editor.fontWeight": "bold",
  "editor.linkedEditing": true,
  "editor.fontLigatures": true,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "expand",
  "editor.guides.bracketPairs": true,
  "editor.stickyScroll.enabled": true,
  "editor.stickyScroll.maxLineCount": 10,
  "editor.inlineSuggest.enabled": true,
  "editor.fontFamily": "Cascadia Code",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.bracketPairColorization.enabled": true,
  "editor.accessibilitySupport": "off",

  "diffEditor.wordWrap": "off",

  "breadcrumbs.enabled": false,

  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "explorer.compactFolders": false,

  "material-icon-theme.folders.color": "#ff0000",
  "material-icon-theme.hidesExplorerArrows": true,

  "window.zoomLevel": -0.25,

  "workbench.tree.indent": 14,
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.editor.showTabs": "multiple",
  "workbench.tree.renderIndentGuides": "none",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.autoLockGroups": {
    "vscode.audioPreview": true,
    "vscode.videoPreview": true
  },
  "workbench.colorCustomizations": {
    "activityBar.border": "#000000",
    "activityBar.foreground": "#ffffff",
    "activityBar.background": "#000000",
    "activityBar.activeBorder": "#ff0000",
    "activityBar.activeBackground": "#000000",
    "activityBarBadge.background": "#ff0000",
    "activityBarBadge.foreground": "#ffffff",

    "sideBar.border": "#000000",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#ffffff99",
    "sideBarTitle.foreground": "#000000",
    "sideBarStickyScroll.border": "#ffffff",
    "sideBarSectionHeader.border": "#ffffff",
    "sideBarSectionHeader.foreground": "#ffffff",

    "scrollbarSlider.background": "#00000050",
    "scrollbarSlider.hoverBackground": "#00000050",
    "scrollbarSlider.activeBackground": "#ff000025",

    "editorGroup.border": "#000000",
    "editorGroup.dropBackground": "#ff000030",
    "editorGroupHeader.border": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorGroupHeader.tabsBorder": "#000000",

    "editor.background": "#000000",
    "editor.selectionBackground": "#ff550075",
    "editor.findMatchBackground": "#000000",
    "editor.lineHighlightBackground": "#ff5e0025",
    "editor.lineHighlightBorder": "#0000",
    "editor.linkedEditingBackground": "#000000",
    "editorLineNumber.activeForeground": "#ff5e00",

    "editorError.background": "#ff000010",
    "editorError.foreground": "#ffffff",
    "editorCursor.background": "#ff0000",
    "editorCursor.foreground": "#ff0000",

    "editorGhostText.foreground": "#ffffff",

    "toolbar.activeBackground": "#000000",
    "toolbar.hoverBackground": "#000000",

    "input.background": "#000000",
    "input.border": "#ffffff25",

    "menu.border": "#ffffff",
    "menu.background": "#000000",
    "menu.foreground": "#ffffff",
    "menu.selectionBorder": "#ff0000",
    "menu.separatorBackground": "#ffffff",
    "menu.selectionBackground": "#ff000050",

    "notificationToast.border": "#ffffff",
    "notifications.border": "#ffffff",
    "notificationsErrorIcon.foreground": "#ff0000",
    "notificationsInfoIcon.foreground": "#0000ff",
    "notificationsWarningIcon.foreground": "#ffff00",
    "notifications.background": "#000000",
    "notifications.foreground": "#ffffff",
    "notificationLink.foreground": "#ff5e00",
    "notificationCenter.border": "#ffffff",

    "quickInput.background": "#000000",
    "quickInput.foreground": "#ffffff",
    "quickInputList.focusBackground": "#ff000050",
    "quickInputList.focusForeground": "#ffffff",

    "radio.activeBorder": "#000000",
    "radio.activeBackground": "#000000",

    "editorSuggestWidget.border": "#ffffff",
    "editorSuggestWidget.background": "#000000",
    "editorSuggestWidget.foreground": "#ffffff",
    "editorSuggestWidget.focusHighlightForeground": "#ff5e00",
    "editorSuggestWidget.selectedIconForeground": "#ffffff",
    "editorSuggestWidget.highlightForeground": "#ff5e00",
    "editorSuggestWidgetStatus.foreground": "#ffffff50",
    "editorSuggestWidget.selectedForeground": "#ffffff",

    "tab.border": "#000000",
    "tab.hoverBorder": "#ff5e00",
    "tab.activeBorder": "#000000",
    "tab.activeBackground": "#000000",
    "tab.activeForeground": "#ffffff",
    "tab.hoverBackground": "#000000",
    "tab.activeBorderTop": "#ff0000",
    "tab.inactiveBackground": "#000000",
    "tab.inactiveForeground": "#ffffff99",

    "titleBar.border": "#000000",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#ffffff99",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.inactiveForeground": "#ffffff99"
  },

  "javascript.updateImportsOnFileMove.enabled": "always",

  "errorLens.enabled": true,

  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": false,
    "javascriptreact": true
  },
  "git.confirmSync": false,
  "git.autofetch": true,
  "doki.sticker.css": "z-index:100;background-position:100% 97%",
  "redhat.telemetry.enabled": true,

  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterNames.enabled": "all",

  "colorize.exclude": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/.git",
    "**/node_modules",
    "**/bower_components",
    "**/tmp",
    "**/dist",
    "**/tests"
  ],
  "colorize.include": [
    "**/*.json",
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "**/*.styl"
  ],

  "vscord.app.name": "Visual Studio Code",
  "vscord.status.idle.enabled": false,
  "vscord.status.problems.text": "- {problems_count} problems",
  "vscord.status.details.text.notInFile": "On VS Code",
  "vscord.status.details.text.idle": "Searching Information!",
  "vscord.status.state.text.viewing": "Editing {file_name}{file_extension}",
  "vscord.status.state.text.editing": "Editing {file_name}{file_extension}",
  "vscord.file.size.round": 5,

  "typescript.inlayHints.parameterTypes.enabled": true,
  "extensions.ignoreRecommendations": true,
  "workbench.tree.enableStickyScroll": true,

  "files.autoSave": "afterDelay",
  "editor.largeFileOptimizations": false,
  "workbench.experimental.share.enabled": true,
  "github.copilot.selectedCompletionModel": "",
  "remote.portsAttributes": {
    "4321": {
      "protocol": "http"
    }
  },
  "workbench.activityBar.location": "top",
  "vscord.status.idle.check": false,
  "vscord.status.idle.timeout": 3000,
  "github.copilot.nextEditSuggestions.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "go.toolsManagement.autoUpdate": true,
  "workbench.editor.empty.hint": "hidden",
  "terminal.integrated.env.windows": {},
  "console-ninja.featureSet": "Community",
  "workbench.colorTheme": "SynthWave '84",
  "workbench.productIconTheme": "fluent-icons",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "workbench.sideBar.location": "right",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.stickyScroll.enabled": false,
  "rapidapi.terminalLink.enabled": false
}